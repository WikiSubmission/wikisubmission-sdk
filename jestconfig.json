{
  "transform": {
    "^.+\\.(t|j)sx?$": ["ts-jest", {
      "tsconfig": "tsconfig.spec.json"
    }]
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "collectCoverageFrom": ["src/**/*.{ts,tsx}"],
  "collectCoverage": true
}